{
  "name": "nomic",
  "version": "0.0.0",
  "description": "",
  "author": "central419",
  "type": "module",
  "scripts": {
    "action": "bun run run-deno ./run/action.ts",
    "check": "bun run run-deno ./run/check.ts",
    "coverage": "deno coverage coverage/profile --ignore=node_modules",
    "knip": "knip",
    "lint": "deno lint",
    "prescience": "bun run run-deno ./run/prescience.ts",
    "progress": "bun run run-deno ./run/progress.ts",
    "run-deno": "deno run --allow-read --allow-env --allow-net=$(paste -d, -s ./allowed_domains)  --allow-write=$(paste -d, -s ./allowed_paths)",
    "test": "deno test -A",
    "verify": "deno check **/*.ts"
  },
  "dependencies": {
    "@octokit/core": "^6.0.1",
    "@octokit/rest": "^20.0.2",
    "immer": "^10.0.4",
    "yaml": "^2.4.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^16.18.87",
    "knip": "^5.0.3",
    "prettier": "^3.2.5",
    "prettier-plugin-jsdoc": "^1.3.0",
    "typescript": "^5.2.2"
  }
}
